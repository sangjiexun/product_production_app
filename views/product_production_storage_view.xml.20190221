<?xml version="1.0"?> 
<odoo>
    <record id="view_form_product_production_storage" model="ir.ui.view">
        <field name="name">製品入庫</field>
        <field name="model">product.production</field>
        <!--<field name="priority">15</field>-->
        <field name="arch" type="xml">
        <form string="product_production">
            <header>
                <button name="button_input_individual" type="object" string="入庫" />
                <button name="button_input_batch" type="object" string="一括入庫" />
            </header>
            <sheet>
                <group name="group_top">
                    <field name="barcode"/>
               </group>
               <group name="group_down">
                    <field name="barcode"/>
               </group>
            </sheet>
        </form>
        </field>
    </record>
</odoo>
